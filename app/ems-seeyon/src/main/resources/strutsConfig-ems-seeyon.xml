<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>
	<form-beans>
		<form-bean name="seeyonForm" type="org.apache.struts.validator.LazyValidatorForm">
		</form-bean>
	</form-beans>

	<action-mappings>
		<action path="/ems/seeyonAction"
				type="com.seeyon.www.action.SeeyonAction"
				name="seeyonForm"
				scope="request" 
				validate="false" 
				parameter="action">
		</action>
	</action-mappings>
</struts-config>